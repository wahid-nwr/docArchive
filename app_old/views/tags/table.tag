<table class="full">
        <thead>
        <tr>
            <th>JiVitA Week</th>
            <th>Sector ID</th>
            <th>HHID</th>
            <th>Name</th>
            <th>Form</th>
        </tr>
    <thead>
    <tbody>
        #{list items:_events, as:'event'}
        <tr>
            <td>${event.start.asJWeek()}</td>
            <td class="aRight">${event.woman.sectorId}</td>
            <td class="aRight">${event.woman.hhId}</td>
            <td>${event.woman.name}</td>
            <td>${event.form?.name}</td>
        </tr>
        #{/list}
        #{else}
        <tr>
            <td colspan="5" class="center">No Schedule Available</td>
        </tr>
        #{/else}
    <tbody>
</table>